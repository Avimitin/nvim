{
  "allow_deadcode": {
    "prefix": "deadcode",
    "body": "#[allow(dead_code)]",
    "description": "Close the dead code warning"
  },
  "closure": {
    "prefix": "||{",
    "body": [
      "|${1:arg}|${2| ,-> |} {",
      "    ${0}",
      "}"
    ]
  },
  "closure as variable": {
    "prefix": "let||",
    "body": [
      "let ${1} = |${2:arg}|${3| ,-> |} {",
      "    ${0}",
      "};"
    ]
  },
  "inline closure": {
    "prefix": "||",
    "body": "|${1: args}| ${0}"
  },
  "Anyhow Result": {
    "prefix": "anyhow",
    "body": "anyhow::Result<()>"
  }
}
